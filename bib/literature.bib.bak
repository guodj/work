@Article{Brinkman2016,
  author                     = {Brinkman, Douglas G. and Walterscheid, Richard L. and Clemmons, James H. and Hecht, James. H.},
  title                      = {High-resolution modeling of the cusp density anomaly: Response to particle and Joule heating under typical conditions},
  journal                    = {J. Geophys. Res.},
  year                       = {2016},
  volume                     = {121},
  pages                      = {2645-2661},
  abstract                   = {{An established high-resolution dynamical model is employed to understand
 the behavior of the thermosphere beneath the Earth's magnetic cusps,
 with emphasis on the factors contributing to the density structures
 observed by the CHAMP and Streak satellite missions. In contrast to
 previous modeling efforts, this approach combines first principles
 dynamical modeling with the high spatial resolution needed to describe
 accurately mesoscale features such as the cusp. The resulting density
 structure is shown to be consistent with observations, including regions
 of both enhanced and diminished neutral density along the satellite
 track. This agreement is shown to be the result of a straightforward
 application of input conditions commonly found in the cusp rather than
 exaggerated or extreme conditions. It is found that the magnitude of the
 density change is sensitive to the width of the cusp region and that
 models that can resolve widths on the order of 2 degrees of latitude are
 required to predict density variations that are consistent with the
 observations.}},
  address                    = {{2000 FLORIDA AVE NW, WASHINGTON, DC 20009 USA}},
  affiliation                = {{Brinkman, DG (Reprint Author), Aerosp Corp, Space Sci Applicat Lab, POB 92957, Los Angeles, CA 90009 USA. Brinkman, Douglas G.; Walterscheid, Richard L.; Clemmons, James H.; Hecht, James. H., Aerosp Corp, Space Sci Applicat Lab, POB 92957, Los Angeles, CA 90009 USA.}},
  author-email               = {{douglas.g.brinkman@aero.org}},
